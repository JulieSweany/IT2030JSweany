@model IEnumerable<EventApplication.Models.EventType>

@{
    ViewBag.Title = "Index";
}

<h2>Event Types</h2>

<p>
    @Html.ActionLink("Back to Organizing Event", "Create", "Events")
</p>
<table class="table">
    <tr>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EventTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EventTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EventTypeId })
        </td>
    </tr>
}

</table>
